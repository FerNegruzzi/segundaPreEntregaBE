<h1>All products</h1>
<ul>
    {{#each products}}
    <li>
        <h2>Title: {{this.title}}</h2>
        <p>Description: {{this.description}}</p>
        <h3>Price: {{this.price}}</h3>
        <p>Category: {{this.category}}</p>
    </li>
    {{/each}}
</ul>

<div>
    {{#if prevLink}}
    <a href="{{prevLink}}">Prev page</a>
    {{/if}}
    {{#if nextLink}}
    <a href="{{nextLink}}">Next page</a>
    {{/if}}
</div>